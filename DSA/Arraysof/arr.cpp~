#include<iostream>
#include<bits/stdc++.h>
using namespace std;
class Arrays{

	public:
	  static void display(int arr[],int size){

		 for(int i=0;i<size;i++){
			 cout<<arr[i]<<" ";
		 }
	}
	  static void maximum(int arr[],int size){

		   int maxi=arr[0];
		   for(int i=0;i<size;i++){
			    if(arr[i]>=maxi){
					 maxi=arr[i];
				}
		   }
		   cout<<"maximum:"<<maxi<<endl;
	  }

	  static void minimum(int arr[],int size){

		   int minx=arr[0];
		   for(int i=0;i<size;i++){
			    if(arr[i]<=minx){
                      minx=arr[i];
				}
		   }
		   cout<<"minimum:"<<minx<<endl;
	  }

	  static long sums(long  long arr[],int size){
		   long long  sum=0;
		   for(long long  i=0;i<size;i++){
			   sum+=arr[i];
		   }
		   
		   return sum;
	  }

	  static void aveg(long long  arr[],int size){

		    int avg=sums(arr,size)/size;

		    cout<<"average:"<<avg<<endl;

	  }

	static void inputArr(){

		long long  arr[10];
		int size;
		int T;

		cin>>T;
		while(T--){

			cin>>size;
			for(long long  i=0;i<size;i++){
				cin>>arr[i];

			} 
		      aveg(arr,size);
		}
	}
};
int main(){

    cout<<"i am coder!";
   
    Arrays::inputArr();	
}
